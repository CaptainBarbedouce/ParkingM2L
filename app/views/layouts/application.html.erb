  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
  <% if current_utilisateur %>
    <div id="user">
      <p>
        <%= link_to current_utilisateur, edit_utilisateur_path(current_utilisateur) %></br>
        <%= link_to 'Profil', edit_utilisateur_path(current_utilisateur) %>
      </p>

      <%= link_to 'DÃ©connexion', destroy_utilisateur_session_path, class: "mv-item-deconnect", method: :delete %>
